		<display-name>Validate email</display-name>
		<validator-id>validateEmail</validator-id>
		<validator-class>com.infy.icci.validator.validateEmail</validator-class>
	</validator>
	<validator>
		<display-name>Validate no symbols, numbers</display-name>
		<validator-id>validateNoSymbolsNoNumbers</validator-id>
		<validator-class>com.infy.icci.validator.NoSymbolsNoNumbers</validator-class>
	</validator>
	<validator>
		<display-name>Validate just numbers</display-name>
		<validator-id>validateNumbers</validator-id>
		<validator-class>com.infy.icci.validator.JustNumbers</validator-class>
	</validator>
	<validator>
		<display-name>Validate calendar</display-name>
		<validator-id>calendarValidate</validator-id>
		<validator-class>com.infy.icci.validator.CalendarValidator</validator-class>
	</validator>
	<validator>
		<validator-id>StringValidator</validator-id>
		<validator-class>com.infy.icci.validator.StringValidator</validator-class>
	</validator>
